<?xml version="1.0" encoding="UTF-8"?>
<xml_registry_redirect hive="HKCR">
	<key name="lnkfile">
		<key name="opus">
			<key name="opentargetcontainer">
				<value name="@default" type="sz">Open Target&apos;s Folder</value>
				<key name="opusflags">
					<value name="Position" type="sz">0</value>
				</key>
				<key name="command">
					<value name="@default" type="sz">go opencontainer=target</value>
				</key>
			</key>
		</key>
	</key>
</xml_registry_redirect>
