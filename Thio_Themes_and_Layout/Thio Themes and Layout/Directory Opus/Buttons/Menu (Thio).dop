		<button backcol="none" display="icon" label_pos="right" textcol="none">
			<label>Dual Horizontal</label>
			<tip>Display two folders (one above the other)</tip>
			<icon1>#dualhorizontal</icon1>
			<function type="normal">
				<instruction>Set DUAL=Toggle,Horiz,Remember,ToggleLayout</instruction>
